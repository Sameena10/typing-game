<body>
    <div class="navbar">
        <div class="logo">
        <a href="">
            <img src="./download.png" alt="" id="logo">
        </a>
    </div>
        <ul class="nav">
            <li class="a"><a href="/">Home</a></li>
            <li class="a"><a href="./second.html">Practice</a></li>
        </ul>
    </div>
    <div class="wrapper">

       <div class="c">
        <input type="text" class="input-field">
        <div class="content-box">
            <div class="typing-text">
                <p id="paragraph"></p>
            </div>
            <div class="content">
                <ul class="result-details">
                    <li class="time">
                        <p>
                            Time Left:
                        </p>
                        <span><b>60</b>s</span>
                    </li>
                    <li class="mistake">
                        <p>
                            Mistakes:
                        </p>
                        <span>0</span>
                    </li>
                    <li class="wpm">
                        <p>
                            WPM:
                        </p>
                        <span>0</span>
                    </li>
                    <li class="cpm">
                        <p>
                            CPM:
                        </p>
                        <span>0</span>
                    </li>
                </ul>
                <button>Try Again</button>
            </div>
        </div>
        </div>
    </div>
